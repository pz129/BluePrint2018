{% extends "base.html.twig" %}
{% block body %}
<div class="full" style="align:center;">
	<h1 class="full" style="text-align:center">
		Submit your file below!
	</h1>
	<form style="width:100%;margin:25px 0px;" method="post" enctype="multipart/form-data">
	<div style="display:flex;flex-direction:row;flex-wrap:wrap;
				justify-content:center;align-items:center;">
	<img style="width:80%;max-width:650px"src="/stocksheets.jpg"/>
	<p style="color:rgb(100,25,25);margin-bottom:25px;text-align:center;width:100%">{{ message }}</p>
	<input id="sound-file" type="file" name="sound-file" > 
	<br><br>
	<input type="submit" class="btn" value="Submit">
	</div>
	</form>
	
</div>
{% endblock body %}