<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
{% block header %}
<style>
@font-face{
font-family:GT-Walsheim;
src:url(GT-Walsheim-Regular.woff);
}
@font-face{
font-family:GT-Walsheim;
src:url(GT-Walsheim-Bold.woff);
font-weight:bold;
}
nav{
width:100%;
background-color:rgb(231, 179, 255);
margin:0px;
}
header{
width:100%;
margin:0px;
}
.full{
width:100%
}
#navbarNav{

}
.navbar-nav li{
border-bottom:0.5px rgb(175,175,175) solid;
}
.btn{
border-bottom:0.5px rgb(175,175,175) solid;
background-color:rgb(231, 179, 255);
}
@media only screen and (min-width: 650px){
 .small{
 	display:none;
 }
 .big{
 	display:block;
 }
 .small-toggle{
 	display:none !important;
 }
}
@media only screen and (max-width: 649px){
	.small{
	display:block;
	}
	.big{
	display:none;
	}
	
}
.list-inline-item .nav-link:hover{
color:rgb(255,255,255);
}
</style>
<div style="padding:0px;" class="navbar navbar-light navbar-toggleable-md bd-navbar">
<nav style="padding:5px;" class="navbar navbar-toggleable-md navbar-light bg-faded">
  <a class="navbar-brand" href="#"><h2>Sheetify</h2></a>
  <div class="big navbar-toggler-right">
   <ul class="list-inline" >
      <li class="list-inline-item active" >
        <a class="nav-link" style="color:rgb(100,100,100)" href="/">Home </a>
      </li>
      <li class="list-inline-item">
        <a class="nav-link" style="color:rgb(100,100,100)" href="/dataset">Find Music</a>
      </li>
      <li class="list-inline-item">
        <a class="nav-link" style="color:rgb(100,100,100)" href="/about-us">About Us</a>
      </li>
    </ul>
    
  </div>
  <button class="navbar-toggler navbar-toggler-right small" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  
</nav>
<div class="collapse navbar-collapse small-toggle" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item active" style="border-top:0.5px rgb(175,175,175) solid;">
        <a class="nav-link" href="/">Home <span class="sr-only">(current)</span></a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/dataset">Find Music</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="/about-us">About Us</a>
      </li>
    </ul>
    </div>
</div>
{% endblock header %}
<div style="margin-top:50px;">
{% block body %}
{% endblock body %}
</div>
<footer>
{% block footer %}

{% endblock footer %}
</footer>
</html>